#!/usr/bin/env zunit

@test 'zinit-annex-bin-gem-node' {
  run zinit load zdharma-continuum/zinit-annex-bin-gem-node
  assert $state equals 0

  local artifact="$ZINIT[PLUGINS_DIR]/zdharma-continuum---zinit-annex-bin-gem-node/z-a-bin-gem-node.plugin.zsh"
  assert "$artifact" is_file
  assert "$artifact" is_readable
}

@test 'zinit-annex-readurl ' {
  run zinit light zdharma-continuum/zinit-annex-readurl

  assert $state equals 0
  assert "$output" contains "Downloading"

  local artifact="$ZINIT[PLUGINS_DIR]/zdharma-continuum---zinit-annex-readurl/z-a-readurl.plugin.zsh"
  assert "$artifact" is_file
  assert "$artifact" is_readable
}

